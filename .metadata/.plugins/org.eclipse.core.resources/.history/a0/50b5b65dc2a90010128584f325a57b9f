#include "maxrefdes24.h"

HAL_StatusTypeDef max24_SetCurrent_Spi1()
{
	HAL_GPIO_WritePin(GPIOC, GPIO_PIN_4, GPIO_PIN_RESET);
	HAL_GPIO_WritePin(GPIOC, GPIO_PIN_5, GPIO_PIN_RESET);
}

HAL_StatusTypeDef max24_SetCurrent_Spi2()
{

}

